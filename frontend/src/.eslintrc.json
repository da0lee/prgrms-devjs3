{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": ["eslint:recommended", "prettier", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": ["import"],
  "rules": {
    "no-console": "warn"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
